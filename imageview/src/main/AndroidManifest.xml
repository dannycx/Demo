<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.danny.imageview">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TestIVActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".ScaleTypeActivity" />
        <activity android:name=".SwitchImageActivity" />
        <activity android:name=".GalleryActivity" />
        <activity android:name=".CacheActivity" />
        <activity android:name=".ShapeActivity" />
        <activity android:name=".BitmapActivity" />
        <activity android:name=".FiveCircleActivity" />
        <activity android:name=".LineActivity" />
        <activity android:name=".picasso.PicassoActivity"/>
    </application>

</manifest>